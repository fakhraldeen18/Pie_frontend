@model Category
<form method="post">
	<div class="border p-3 mt-4">
		<div class="row pb-2">
			<h2 class="text-primary">Delete Category</h2>
			<hr />
		</div>
		<div class="mb-3">
			<label asp-for="CategoryName"></label>
			<input asp-for="CategoryName" disabled class="form-control" />
		</div>

		<div class="mb-3">
			<label asp-for="Description"></label>
			<input asp-for="Description" disabled class="form-control" />
		</div>
		<div class="row">
			<div class="col-6">
				<button class="btn btn-primary rounded-2  form-control" type="submit">Delete</button>
			</div>
			<div class="col-6">
				<a asp-controller="Dashboard" asp-action="Index" class="btn btn-outline-secondary rounded-2 form-control
">Back to List</a>

			</div>

		</div>

	</div>
</form>